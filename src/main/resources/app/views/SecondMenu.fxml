<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<!--
These are the components which represent the second menu.
-->

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.SecondMenuController">
   <children>
      <Button layoutX="513.0" layoutY="325.0" mnemonicParsing="false" onAction="#goBack" text="back" />
      <Text layoutX="252.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="This is the second menu">
         <font>
            <Font size="30.0" />
         </font>
      </Text>
      <ListView fx:id="_listView" editable="true" layoutX="30.0" layoutY="31.0" prefHeight="325.0" prefWidth="200.0" />
      <Button layoutX="453.0" layoutY="325.0" mnemonicParsing="false" onAction="#updateList" text="add" />
      <TextField fx:id="_inputField" layoutX="252.0" layoutY="325.0" />
   </children>
</AnchorPane>
